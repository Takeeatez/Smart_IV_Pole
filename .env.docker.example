# Docker Compose Environment Variables
# Copy to .env.docker and fill in your credentials

# MariaDB Configuration
MYSQL_ROOT_PASSWORD=your_root_password
MYSQL_DATABASE=smart_iv_pole
MYSQL_USER=smart_iv_user
MYSQL_PASSWORD=your_secure_password

# Backend Configuration (if using docker backend service)
SPRING_DATASOURCE_URL=jdbc:mariadb://mariadb:3306/smart_iv_pole
SPRING_DATASOURCE_USERNAME=smart_iv_user
SPRING_DATASOURCE_PASSWORD=your_secure_password
MQTT_BROKER_URL=tcp://mosquitto:1883
